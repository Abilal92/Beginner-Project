// FactoriAL

#include <iostream>

using namespace std;

int main() {
   
   int number;
   int factorial=1;
   
   cout<<"Enter number: ";
   cin >> number;
   
   //1st method
   for(int i=1;i<= number;i++){
       factorial=factorial*i;
   }
   
   //2nd method
 /*  for(int i=number;i>=1;i--){
       factorial=factorial*i;
   } */
   
   cout << number << "!=" << factorial;
    
    return 0;
}
